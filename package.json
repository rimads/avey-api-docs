{
  "name": "api-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "gen:all": "npm run gen:procedure && npm run gen:diagnosis && npm run gen:drug && npm run gen:cpt && npm run gen:icd && npm run gen:ndc",
    "gen:procedure": "node ./scripts/generate-docs.mjs procedure",
    "gen:diagnosis": "node ./scripts/generate-docs.mjs diagnosis",
    "gen:drug": "node ./scripts/generate-docs.mjs drug",
    "gen:cpt": "node ./scripts/generate-docs.mjs cpt",
    "gen:icd": "node ./scripts/generate-docs.mjs icd",
    "gen:ndc": "node ./scripts/generate-docs.mjs ndc"
  },
  "dependencies": {
    "fumadocs-core": "15.6.6",
    "fumadocs-mdx": "11.7.1",
    "fumadocs-openapi": "^9.1.6",
    "fumadocs-ui": "15.6.6",
    "mermaid": "^11.9.0",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "shiki": "^3.9.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^8",
    "eslint-config-next": "15.4.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  }
}
