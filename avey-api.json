{
  "openapi": "3.1.0",
  "info": {
    "title": "Avey Diagnosis API",
    "description": "API for performing medical diagnoses",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "http://35.188.153.196:8000",
      "description": "Production"
    }
  ],
  "paths": {
    "/sessions": {
      "post": {
        "summary": "New Session",
        "operationId": "new_session_sessions_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/NewSessionRequest" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/NewSessionResponse" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/diagnose": {
      "post": {
        "summary": "New Diagnosis",
        "operationId": "new_diagnosis_diagnose_post",
        "parameters": [
          {
            "name": "next_steps",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Next Steps"
            }
          },
          {
            "name": "use_phoenix",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Use Phoenix"
            }
          },
          {
            "name": "use_lm",
            "in": "query",
            "required": false,
            "schema": { "type": "boolean", "default": true, "title": "Use Lm" }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/DiagnoseRequest" }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/DiagnoseResponse" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/sessions/{session_id}": {
      "post": {
        "summary": "Update Session",
        "operationId": "update_session_sessions__session_id__post",
        "parameters": [
          {
            "name": "session_id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "title": "Session Id" }
          },
          {
            "name": "advice",
            "in": "query",
            "required": false,
            "schema": { "type": "boolean", "default": true, "title": "Advice" }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/UpdateSessionRequest" }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UpdateSessionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CodingType": {
        "type": "string",
        "enum": ["icd-10-cm", "snomed-ct", "avey-kr"],
        "title": "CodingType"
      },
      "Concept": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "term": { "type": "string", "title": "Term" }
        },
        "type": "object",
        "required": ["id", "term"],
        "title": "Concept"
      },
      "DiagnoseRequest": {
        "properties": {
          "patient_info": { "$ref": "#/components/schemas/PatientInfo-Input" }
        },
        "type": "object",
        "required": ["patient_info"],
        "title": "DiagnoseRequest"
      },
      "DiagnoseResponse": {
        "properties": {
          "assessment": { "$ref": "#/components/schemas/PatientAssessment" }
        },
        "type": "object",
        "required": ["assessment"],
        "title": "DiagnoseResponse"
      },
      "Disease": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "description": { "type": "string", "title": "Description" },
          "term": { "type": "string", "title": "Term" }
        },
        "type": "object",
        "required": ["id", "description", "term"],
        "title": "Disease"
      },
      "Finding": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "term": { "type": "string", "title": "Term" },
          "status": { "$ref": "#/components/schemas/FindingStatus" }
        },
        "type": "object",
        "required": ["id", "term", "status"],
        "title": "Finding"
      },
      "FindingAnswer": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "status": { "$ref": "#/components/schemas/FindingStatus" }
        },
        "type": "object",
        "required": ["id", "status"],
        "title": "FindingAnswer"
      },
      "FindingStatus": {
        "type": "string",
        "enum": ["present", "absent", "unsure"],
        "title": "FindingStatus"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ImageQuestion": {
        "properties": {
          "text": { "type": "string", "title": "Text" },
          "selections": {
            "items": { "$ref": "#/components/schemas/ImageQuestionSelection" },
            "type": "array",
            "title": "Selections"
          }
        },
        "type": "object",
        "required": ["text", "selections"],
        "title": "ImageQuestion"
      },
      "ImageQuestionSelection": {
        "properties": {
          "selected_image_url": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Selected Image Url"
          },
          "unselected_image_url": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Unselected Image Url"
          },
          "selection_id": { "type": "string", "title": "Selection Id" }
        },
        "type": "object",
        "required": ["selection_id"],
        "title": "ImageQuestionSelection"
      },
      "NewSessionRequest": {
        "properties": {
          "coding_type": { "$ref": "#/components/schemas/CodingType" },
          "allow_images": {
            "type": "boolean",
            "title": "Allow Images",
            "default": false
          },
          "patient_info": { "$ref": "#/components/schemas/PatientInfoRequest" },
          "input_text": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Input Text",
            "description": "Only for enterprise tier"
          }
        },
        "type": "object",
        "required": ["coding_type", "patient_info"],
        "title": "NewSessionRequest"
      },
      "NewSessionResponse": {
        "properties": {
          "session_id": { "type": "string", "title": "Session Id" },
          "question": { "$ref": "#/components/schemas/Question" },
          "patient_info": { "$ref": "#/components/schemas/PatientInfo-Output" }
        },
        "type": "object",
        "required": ["session_id", "question", "patient_info"],
        "title": "NewSessionResponse"
      },
      "PatientAssessment": {
        "properties": {
          "differential_diagnosis": {
            "items": { "$ref": "#/components/schemas/Disease" },
            "type": "array",
            "title": "Differential Diagnosis"
          },
          "advice": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Advice"
          }
        },
        "type": "object",
        "required": ["differential_diagnosis"],
        "title": "PatientAssessment"
      },
      "PatientInfo-Input": {
        "properties": {
          "age": { "type": "integer", "title": "Age" },
          "sex": { "$ref": "#/components/schemas/Sex" },
          "chief_findings": {
            "items": { "$ref": "#/components/schemas/Finding" },
            "type": "array",
            "title": "Chief Findings"
          },
          "findings": {
            "items": { "$ref": "#/components/schemas/Finding" },
            "type": "array",
            "title": "Findings"
          }
        },
        "type": "object",
        "required": ["age", "sex", "chief_findings", "findings"],
        "title": "PatientInfo"
      },
      "PatientInfo-Output": {
        "properties": {
          "age": { "type": "integer", "title": "Age" },
          "sex": { "$ref": "#/components/schemas/Sex" },
          "chief_findings": {
            "items": { "$ref": "#/components/schemas/Finding" },
            "type": "array",
            "title": "Chief Findings"
          },
          "findings": {
            "items": { "$ref": "#/components/schemas/Finding" },
            "type": "array",
            "title": "Findings"
          }
        },
        "type": "object",
        "required": ["age", "sex", "chief_findings", "findings"],
        "title": "PatientInfo"
      },
      "PatientInfoRequest": {
        "properties": {
          "age": { "type": "integer", "title": "Age" },
          "sex": { "$ref": "#/components/schemas/Sex" },
          "chief_findings_codes": {
            "anyOf": [
              { "items": { "type": "string" }, "type": "array" },
              { "type": "null" }
            ],
            "title": "Chief Findings Codes"
          }
        },
        "type": "object",
        "required": ["age", "sex"],
        "title": "PatientInfoRequest"
      },
      "Question": {
        "properties": {
          "type": { "$ref": "#/components/schemas/QuestionType" },
          "id": { "type": "integer", "title": "Id" },
          "content": {
            "anyOf": [
              { "$ref": "#/components/schemas/ImageQuestion" },
              { "$ref": "#/components/schemas/TextQuestion" }
            ],
            "title": "Content"
          }
        },
        "type": "object",
        "required": ["type", "id", "content"],
        "title": "Question"
      },
      "QuestionType": {
        "type": "string",
        "enum": ["image", "text"],
        "title": "QuestionType"
      },
      "Sex": { "type": "string", "enum": ["male", "female"], "title": "Sex" },
      "TextQuestion": {
        "properties": {
          "text": { "type": "string", "title": "Text" },
          "target_concept": { "$ref": "#/components/schemas/Concept" }
        },
        "type": "object",
        "required": ["text", "target_concept"],
        "title": "TextQuestion"
      },
      "UpdateSessionRequest": {
        "properties": {
          "question_id": { "type": "integer", "title": "Question Id" },
          "answers": {
            "items": { "$ref": "#/components/schemas/FindingAnswer" },
            "type": "array",
            "title": "Answers"
          },
          "image_answers": {
            "anyOf": [
              {
                "additionalProperties": {
                  "$ref": "#/components/schemas/FindingStatus"
                },
                "type": "object"
              },
              { "type": "null" }
            ],
            "title": "Image Answers"
          },
          "input_text": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Input Text",
            "description": "Only for enterprise tier"
          }
        },
        "type": "object",
        "required": ["question_id", "answers"],
        "title": "UpdateSessionRequest"
      },
      "UpdateSessionResponse": {
        "properties": {
          "type": { "$ref": "#/components/schemas/UpdateSessionResponseType" },
          "data": {
            "anyOf": [
              { "$ref": "#/components/schemas/Question" },
              { "$ref": "#/components/schemas/PatientAssessment" }
            ],
            "title": "Data"
          },
          "patient_info": { "$ref": "#/components/schemas/PatientInfo-Output" }
        },
        "type": "object",
        "required": ["type", "data", "patient_info"],
        "title": "UpdateSessionResponse"
      },
      "UpdateSessionResponseType": {
        "type": "string",
        "enum": ["question", "assessment"],
        "title": "UpdateSessionResponseType"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
